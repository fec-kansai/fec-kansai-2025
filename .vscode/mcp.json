{
  "servers": {
    "figma-developer-mcp": {
      "command": "npx",
      "args": ["-y", "figma-developer-mcp", "--stdio"],
      "env": {
        "FIGMA_API_KEY": "${input:FIGMA_API_KEY}"
      }
    }
  },
  "inputs": [
    {
      "id": "FIGMA_API_KEY",
      "description": "Figma API key",
      "type": "promptString",
      "password": true
    }
  ]
}
